wget https://raw.githubusercontent.com/Sunlight-Warrior/Big-Data/main/dados/original.csv -O original_raw.csv && iconv -f ISO-8859-1 -t UTF-8 original_raw.csv -o original_utf8.csv


Invoke-WebRequest "https://raw.githubusercontent.com/Sunlight-Warrior/Big-Data/main/dados/original.csv" -OutFile original_raw.csv
Get-Content original_raw.csv -Encoding Latin1 | Set-Content original_utf8.csv -Encoding UTF8
